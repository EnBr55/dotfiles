URxvt.intensityStyles:	false
URxvt.background:       [88]#120512
URxvt.depth:		32
URxvt.font:		xft:FiraCode-Regular:size=14,xft:Noto Emoji, xft:FontAwesome
URxvt.scrollBar:	false
URxvt.cursorColor:	white

!! Copy pasting with ctrl + shift + c/v
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

!! Disable previous bindings for ctrl + shift + c/v
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Extensions
urxvt.perl-ext-common: default,tabbed,matcher,resize-font,-tabbed
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

